<div id="page-wrapper">
  <div class="container-fluid">
    <div class="row bg-title">
      <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
        <h4 class="page-title">Issue Details</h4></div>
      <!-- /.col-lg-12 -->
    </div>
    <div class="col-sm-6">
      <div class="white-box">
        <div class="table-responsive">
          <table class="table color-table primary-table">
            <thead>
            <tr>
              <th>#</th>
              <th>First Name</th>
              <th>Last Name</th>
              <th>Order Number</th>
              <th>Description</th>
            </tr>
            </thead>
            <tbody>
            <tr>
              <td><%= @dispute.id %></td>
              <td><%= @dispute.first_name %></td>
              <td><%= @dispute.last_name %></td>
              <td><%= @dispute.order_number %></td>
              <td><%= @dispute.description %></td>
            </tr>

            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>